{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/index.ts"
    },
    {
      "source": "/:path*",
      "destination": "/dist/client/:path*"
    }
  ]
}
